   "source": [
    "class Solution:\n",
    "    def reverseStr(self, s: str, k: int) -> str:\n",
    "        L = len(s)\n",
    "        if L < k: return s[::-1]\n",
    "        elif k <= L < 2*k: return (s[:k][::-1]+s[k:])\n",
    "        else: \n",
    "            return s[:k][::-1]+s[k:2*k]+self.reverseStr(s[2*k:],k)"
   ]
  }
